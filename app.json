{
  "expo": {
    "name": "Self Blog",
    "slug": "rn-self-posts",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "description": "A simple Self Posts app with various features and very creative design adapted to each platform in its own way",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fufylevdev.self-blog-rn-app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.fufylevdev.self_blog_rn_app",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "WRITE_EXTERNAL_STORAGE"
      ]
    }
  }
}
